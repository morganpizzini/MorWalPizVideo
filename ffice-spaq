alias.alias=! git config --get-regexp ^alias\. | sed -e s/^alias\.// -e s/\ /\ =\ /
alias.cmp=!f() { git add -A && git commit -m "$1" && git push; }; f
alias.lg=log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
alias.amend=!f() { git add -A && git commit --amend --no-edit && git push -f; }; f
alias.start=!f() { git reset --hard && git checkout main && git pull && git checkout -b $1 && git push -u origin $1; }; f
alias.co=checkout
alias.br=branch
alias.st=status
alias.what=for-each-ref --sort=-committerdate refs/heads/ --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) - %(color:red)%(objectname:short)%(color:reset) - %(contents:subject) (%(color:green)%(committerdate:relative)%(color:reset)) %(color:blue)<%(authorname)>%(color:reset)'
alias.newb=!f() { git reset --hard && git checkout -b sample && git branch -D main && git fetch && git checkout main && git branch -D sample && git checkout -b $1 && git push -u origin $1; }; f
alias.squash=!f() { git reset --soft HEAD~$1 && git commit -m "$2"; }; f
